{
  "name": "myqa-webapp",
  "version": "1.0.0",
  "description": "Myqa web app",
  "scripts": {
    "check-types": "tsc --noEmit --skipLibCheck",
    "lint": "eslint --ext .ts,tsx src",
    "lint-fix": "eslint --ext .ts,tsx --fix src",
    "lint-staged": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node dist/server/index.js",
    "dev": "nodemon --ignore 'dist/**' --watch 'src' -e ts,js,tsx,jsx --exec 'npm run build-serve'",
    "dev-client": "DEVELOPMENT_JUST_CLIENT=true parcel src/ui/index.html",
    "dev-server": "DEVELOPMENT_JUST_SERVER=true babel-watch -e ['.js','.jsx','.ts','.tsx','.json'] src/server/index.ts",
    "build-serve": "parcel build --no-optimize && npm run serve",
    "build": "parcel build",
    "prepare": "husky install",
    "pre-commit": "npm run lint-staged && npm run check-types"
  },
  "targets": {
    "client": {
      "source": "./src/ui/index.tsx",
      "distDir": "./dist/client",
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      },
      "publicUrl": "/dist/client"
    },
    "server": {
      "source": "./src/server/index.ts",
      "distDir": "./dist/server",
      "engines": {
        "node": "14"
      },
      "publicUrl": "/dist/client"
    }
  },
  "keywords": [
    "web",
    "app",
    "trading",
    "biscuits",
    "utilities",
    "utils",
    "stock",
    "crypto",
    "investing"
  ],
  "author": "Joao Macedo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@hyqo/parcel-reporter-asset-map": "^1.0.3",
    "@parcel/resolver-glob": "^2.8.3",
    "@parcel/transformer-image": "^2.8.3",
    "@redux-devtools/core": "^3.13.1",
